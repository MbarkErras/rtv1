camera: [x,y,z] [x,y,z]
		postion direction
light: [x,y,z] [intensity] [c]
	   position intensity

sphere: [x,y,z] [x,y,z] [c] [r]
		positon translation

plane: [x,y,z] [x,y,z] [x,y,z] [x,y,z] [c]
	   translation direction rotation position color 

cylinder: [x,y,z] [x,y,z]  [x,y,z] [x,y,z] [c] [r]
		  position direction translation rotation radius
cone: [x,y,z] [x,y,z] [x,y,z] [x,y,z] [x,y,z] [c]
	  position direction translation rotation angle
